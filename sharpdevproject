local spinbottoggle = Misc:CreateToggle({
    Name = "Spin-Bot",
    CurrentValue = false,
    Flag = "SpinBot",
    Callback = function(Value)
        spinBot = Value
        if Value then
            for i,v in pairs(hrp:GetChildren()) do
                if v.Name == "Spinning" then
                    v:Destroy()
                end
            end
            plr.Character.Humanoid.AutoRotate = false
            local Spin = Instance.new("BodyAngularVelocity")
            Spin.Name = "Spinning"
            Spin.Parent = hrp
            Spin.MaxTorque = Vector3.new(0, math.huge, 0)
            Spin.AngularVelocity = Vector3.new(0,spinBotSpeed,0)
            Rayfield:Notify({Title = "Spin Bot", Content = "Enabled!", Duration = 1, Image = 4483362458,})
        else
            for i,v in pairs(hrp:GetChildren()) do
                if v.Name == "Spinning" then
                    v:Destroy()
                end
            end
            plr.Character.Humanoid.AutoRotate = true
            Rayfield:Notify({Title = "Spin Bot", Content = "Disabled!", Duration = 1, Image = 4483362458,})
        end
    end
})

local spinbotspeed = Misc:CreateSlider({
    Name = "Spin-Bot Speed",
    Range = {0, 1000},
    Increment = 1,
    CurrentValue = 20,
    Flag = "SpinBotSpeed",
    Callback = function(Value)
        spinBotSpeed = Value
        if spinBot then
            for i,v in pairs(hrp:GetChildren()) do
                if v.Name == "Spinning" then
                    v:Destroy()
                end
            end
            local Spin = Instance.new("BodyAngularVelocity")
            Spin.Name = "Spinning"
            Spin.Parent = hrp
            Spin.MaxTorque = Vector3.new(0, math.huge, 0)
            Spin.AngularVelocity = Vector3.new(0,Value,0)
        end
    end,
})

local ServerHop = Misc:CreateButton({
	Name = "Server Hop",
	Callback = function()
		if httprequest then
            local servers = {}
            local req = httprequest({Url = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Desc&limit=100&excludeFullGames=true", game.PlaceId)})
            local body = HttpService:JSONDecode(req.Body)
        
            if body and body.data then
                for i, v in next, body.data do
                    if type(v) == "table" and tonumber(v.playing) and tonumber(v.maxPlayers) and v.playing < v.maxPlayers and v.id ~= game.JobId then
                        table.insert(servers, 1, v.id)
                    end
                end
            end
        
            if #servers > 0 then
                TeleportService:TeleportToPlaceInstance(game.PlaceId, servers[math.random(1, #servers)], plr)
            else
                Rayfield:Notify({Title = "Server Hop", Content = "Couldn't find a valid server!!!", Duration = 1, Image = 4483362458,})
            end
        else
            Rayfield:Notify({Title = "Server Hop", Content = "Your executor is ass!", Duration = 1, Image = 4483362458,})
        end
	end,
})

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

-- Drawings and State
local espBoxes, espTracers, espDistanceTexts, espHealthBars = {}, {}, {}, {}, {}
local manuallyWhitelisted = {}

-- Settings
local espEnabled = false
local showTracers = false
local showDistance = false
local showHealthBar = false
local boxSize = Vector2.new(60, 100)

-- Functions
local function createESP(player)
    if player == LocalPlayer or espBoxes[player] then return end

    espBoxes[player] = Drawing.new("Square")
    espBoxes[player].Thickness = 1
    espBoxes[player].Size = boxSize
    espBoxes[player].Color = Color3.fromRGB(255, 0, 0)
    espBoxes[player].Filled = false
    espBoxes[player].Visible = false

    espTracers[player] = Drawing.new("Line")
    espTracers[player].Thickness = 1
    espTracers[player].Color = Color3.fromRGB(0, 255, 0)
    espTracers[player].Visible = false

    espDistanceTexts[player] = Drawing.new("Text")
    espDistanceTexts[player].Size = 14
    espDistanceTexts[player].Color = Color3.fromRGB(255, 255, 255)
    espDistanceTexts[player].Outline = true
    espDistanceTexts[player].Visible = false

    espHealthBars[player] = Drawing.new("Line")
    espHealthBars[player].Thickness = 2
    espHealthBars[player].Color = Color3.fromRGB(0, 255, 0)
    espHealthBars[player].Visible = false
end

local function removeESP(player)
    if espBoxes[player] then espBoxes[player]:Remove() espBoxes[player] = nil end
    if espTracers[player] then espTracers[player]:Remove() espTracers[player] = nil end
    if espDistanceTexts[player] then espDistanceTexts[player]:Remove() espDistanceTexts[player] = nil end
    if espHealthBars[player] then espHealthBars[player]:Remove() espHealthBars[player] = nil end
end

local function updateESP()
    for player, box in pairs(espBoxes) do
        local char = player.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        local hum = char and char:FindFirstChildOfClass("Humanoid")

        if hrp and hum and hum.Health > 0 and not manuallyWhitelisted[player.Name] then
            local screenPos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
            box.Position = Vector2.new(screenPos.X - boxSize.X / 2, screenPos.Y - boxSize.Y / 2)
            box.Visible = onScreen

            -- Tracer
            local tracer = espTracers[player]
            if showTracers then
                tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
                tracer.To = Vector2.new(screenPos.X, screenPos.Y)
                tracer.Visible = onScreen
            else
                tracer.Visible = false
            end

            -- Distance
            local distance = (Camera.CFrame.Position - hrp.Position).Magnitude
            local text = espDistanceTexts[player]
            if showDistance then
                text.Text = math.floor(distance) .. "m"
                text.Position = Vector2.new(screenPos.X - boxSize.X / 2, screenPos.Y + boxSize.Y / 2 + 2)
                text.Visible = onScreen
            else
                text.Visible = false
            end

            -- Health
            local bar = espHealthBars[player]
if showHealthBar then
    local ratio = math.clamp(hum.Health / hum.MaxHealth, 0, 1)
    local fullHeight = boxSize.Y
    local height = fullHeight * ratio
    local topY = screenPos.Y - fullHeight / 2
    local bottomY = topY + height

    bar.From = Vector2.new(screenPos.X - boxSize.X / 2 - 4, bottomY)
    bar.To = Vector2.new(screenPos.X - boxSize.X / 2 - 4, topY)
    bar.Color = Color3.fromRGB(255 * (1 - ratio), 255 * ratio, 0) -- red to green
    bar.Visible = onScreen
else
    bar.Visible = false
end

        else
            box.Visible = false
            espTracers[player].Visible = false
            espDistanceTexts[player].Visible = false
            espHealthBars[player].Visible = false
        end
    end
end

-- ESP render step
local renderConnection

-- ESP Toggle
local espToggle = Misc:CreateToggle({
    Name = "ESP",
    CurrentValue = false,
    Flag = "ESP_Toggle",
    Callback = function(Value)
        espEnabled = Value
        if Value then
            for _, player in ipairs(Players:GetPlayers()) do createESP(player) end
            Players.PlayerAdded:Connect(createESP)
            Players.PlayerRemoving:Connect(removeESP)
            renderConnection = RunService.RenderStepped:Connect(updateESP)
        else
            if renderConnection then renderConnection:Disconnect() end
            for _, d in pairs(espBoxes) do d:Remove() end
            for _, d in pairs(espTracers) do d:Remove() end
            for _, d in pairs(espDistanceTexts) do d:Remove() end
            for _, d in pairs(espHealthBars) do d:Remove() end
            espBoxes, espTracers, espDistanceTexts, espHealthBars = {}, {}, {}, {}
        end
    end
})

-- Tracer Toggle
local tracerToggle = Misc:CreateToggle({
    Name = "ESP Tracers",
    CurrentValue = false,
    Flag = "ESP_Tracers",
    Callback = function(Value)
        showTracers = Value
    end
})

-- Distance Toggle
local distanceToggle = Misc:CreateToggle({
    Name = "Show Distance",
    CurrentValue = false,
    Flag = "ESP_Distance",
    Callback = function(Value)
        showDistance = Value
    end
})

-- Health Bar Toggle
local healthToggle = Misc:CreateToggle({
    Name = "Show Health Bars",
    CurrentValue = false,
    Flag = "ESP_Health",
    Callback = function(Value)
        showHealthBar = Value
    end
})

-- Whitelist Dropdown
local whitelistDropdown = Misc:CreateDropdown({
    Name = "ESP Whitelist",
    Options = {},
    CurrentOption = {},
    MultipleOptions = true,
    Flag = "ESPWhitelist",
    Callback = function(Options)
        manuallyWhitelisted = {}
        for _, name in ipairs(Options) do manuallyWhitelisted[name] = true end
    end,
})

-- Refresh whitelist options every 2 minutes
task.spawn(function()
    while true do
        local names = {}
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then table.insert(names, player.Name) end
        end
        if whitelistDropdown then whitelistDropdown:Refresh(names, true) end
        task.wait(120)
    end
end)


local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera
local UserInputService = game:GetService("UserInputService")

-- Initial Settings
local flyEnabled = false
local flySpeed = 50
local jumpBoost = 50
local isFlying = false
local velocity = Vector3.new()

-- Create Sliders for Speed and Jump Boost under the Pierdolinko tab
local speedSlider = Pierdolinko:CreateSlider({
   Name = "Fly Speed",
   Range = {0, 200},  -- Can adjust range based on your needs
   Increment = 5,
   Suffix = "Speed",
   CurrentValue = 50,  -- Default fly speed
   Flag = "FlySpeed",
   Callback = function(Value)
       flySpeed = Value
   end,
})

local jumpSlider = Pierdolinko:CreateSlider({
   Name = "Jump Boost",
   Range = {0, 200},  -- Adjust to your needs
   Increment = 5,
   Suffix = "Jump",
   CurrentValue = 50,  -- Default jump boost
   Flag = "JumpBoost",
   Callback = function(Value)
       jumpBoost = Value
   end,
})

-- Fly function to enable/disable flying
local function toggleFly()
    if flyEnabled then
        -- Start flying
        if not isFlying then
            isFlying = true
            velocity = Vector3.new(0, jumpBoost, 0) -- Initialize the jump boost
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.MaxForce = Vector3.new(400000, 400000, 400000)
            bodyVelocity.Velocity = velocity
            bodyVelocity.Parent = LocalPlayer.Character.HumanoidRootPart
            local bodyGyro = Instance.new("BodyGyro")
            bodyGyro.MaxTorque = Vector3.new(400000, 400000, 400000)
            bodyGyro.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
            bodyGyro.Parent = LocalPlayer.Character.HumanoidRootPart
        end
    else
        -- Stop flying
        if isFlying then
            isFlying = false
            for _, v in pairs(LocalPlayer.Character.HumanoidRootPart:GetChildren()) do
                if v:IsA("BodyVelocity") or v:IsA("BodyGyro") then
                    v:Destroy()
                end
            end
        end
    end
end

-- Fly Toggle
local flyToggle = Pierdolinko:CreateToggle({
   Name = "Fly",
   CurrentValue = false,
   Flag = "Fly_Toggle",
   Callback = function(Value)
       flyEnabled = Value
       toggleFly()  -- Call the function when toggle is pressed
   end,
})

-- Update Fly Speed during flight
RunService.RenderStepped:Connect(function()
    if isFlying then
        local humanoidRootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            -- Adjust the velocity based on the flySpeed and direction
            velocity = Vector3.new(0, jumpBoost, flySpeed)
            humanoidRootPart.BodyVelocity.Velocity = velocity
        end
    end
end)

-- Jump Boost functionality
UserInputService.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Space then
        if isFlying then
            -- Apply jump boost while flying
            velocity = Vector3.new(0, jumpBoost, 0)  -- Reset vertical velocity for jump boost
        end
    end
end)
